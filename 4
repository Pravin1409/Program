#include<stdio.h>
int DisplayDigit(char *str)
{

    while (*str!='\0')
    {
        if ((*str>='0')&&(*str<='9'))
       {


       }
    }
     
    
}
int main()
{
    char arr[30];
    int iRet=0;

    printf("Enter the string:");
    scanf("%[^'\n']s",arr);

    iRet=DisplayDigit(arr);
    printf("%d",iRet);

    return 0;
}